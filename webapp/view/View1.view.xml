											<TextArea value="{/CmComments}" id="idCmComments" growing="true" width="100%" maxLength="250" enabled="{localModel>/idCmComments}"/>
											<!--<core:Title text="CM Comments 2"/>-->
											<!--<TextArea value="{/CmComments2}" id="idCmComments2" growing="true" width="100%" maxLength="250" enabled="{localModel>/CmComments2}"/>-->
											<core:Title text="Credit Control Comments" required="true"/>
											<TextArea value="{/CcComments}" id="idCcComments" growing="true" width="100%" maxLength="250" enabled="{localModel>/idCcComments}"/>
											<!--<core:Title text="CC Comments 2"/>-->
